<Window x:Class="WPFPunchCard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:wpfPunchCard="clr-namespace:WPFPunchCard"
        Title="MainWindow" Height="350" Width="525" DataContext="{Binding Mode=OneWay, RelativeSource={RelativeSource Self}}">
    <DockPanel>
        <wpfPunchCard:PunchCard Data="{Binding Data}" />
    </DockPanel>
</Window>
